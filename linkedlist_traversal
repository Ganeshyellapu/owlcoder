#include<bits/stdc++.h>
using namespace std;
struct Node{
    char data;
    Node *next;
    Node(int x){
        data=x;
        next=NULL;
    }
};
void printlist(Node *head){
    Node *curr=head;
    while(curr!=NULL){
        cout<<curr->data<<" ";
        curr=curr->next;
    }
}
int main()
{
    Node *head =new Node(97);
    head->next=new Node(98);
    head->next->next=new Node(105);
    head->next->next->next=new Node(100);
    printlist(head);
    return 0;
}
